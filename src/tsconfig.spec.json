      ],
      "@env": [
        "src/environments/"
      ],
      "@env/*": [
        "src/environments/*"
      ],
      "@Framework": [
        "D:/PoYa/Project/AngularFramework/@Framework/"
      ],
      "@Framework/*": [
        "D:/PoYa/Project/AngularFramework/@Framework/*"
      ],
      "@Common": [
        "D:/PoYa/Project/AngularFramework/@Common/"
