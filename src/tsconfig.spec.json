      "@Common/*": [
        "D:/PoYa/Project/AngularFramework/@Common/*"
      ],
      "@FrameworkPage": [
        "D:/PoYa/Project/AngularFramework/@FrameworkPage/"
      ],
      "@FrameworkPage/*": [
        "D:/PoYa/Project/AngularFramework/@FrameworkPage/*"
      ]

    }
  },
  "files": [
    "test.ts",
    "polyfills.ts",
    "D:/PoYa/Project/AngularFramework/@Core/*.ts",
    "D:/PoYa/Project/AngularFramework/@Shared/*.ts",
    "D:/PoYa/Project/AngularFramework/@Framework/index.ts",
    "D:/PoYa/Project/AngularFramework/@FrameworkPage/index.ts",
    ],
  "include": [
    "**/*.spec.ts",
    "**/*.d.ts"
  ]
}
