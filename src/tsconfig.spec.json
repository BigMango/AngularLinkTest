{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/spec",
    "baseUrl": "./",
    "module": "commonjs",
    "target": "es5",
    "types": [
      "jasmine",
      "node"
    ],
    "paths": {
      "@shared": [
        "D:/PoYa/Project/AngularFramework/@Shared"
      ],
      "@shared/*": [
        "D:/PoYa/Project/AngularFramework/@Shared/*"
      ],
      "@core": [
        "D:/PoYa/Project/AngularFramework/@Core"
      ],
      "@core/*": [
        "D:/PoYa/Project/AngularFramework/@Core/*"
      ],
      "@testing": [
        "testing/"
      ],
      "@testing/*": [
